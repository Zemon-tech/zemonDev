{
  "name": "quild",
  "compatibility_date": "2024-01-01",
  "compatibility_flags": ["nodejs_compat"],
  "pages": {
    "build": {
      "command": "npm run build",
      "output_dir": "dist"
    }
  },
  "env": {
    "production": {
      "vars": {
        "NODE_ENV": "production"
      }
    }
  },
  "routes": [
    {
      "pattern": "/api/*",
      "script": "api/*"
    }
  ],
  "redirects": [
    {
      "from": "/*",
      "to": "/index.html",
      "status": 200
    }
  ]
}
